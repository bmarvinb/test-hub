{
  "name": "test-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --ext .ts,.tsx ./src --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .ts,.tsx ./src --no-error-on-unmatched-pattern --fix",
    "prettier": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "types": "tsc --noEmit --pretty --watch",
    "types:check": "tsc --noEmit --pretty",
    "format": "prettier -w ./src",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "i18n:migrate": "locize migrate --path=./src/app/i18n/locales",
    "i18n:download": "locize download --ver=latest --clean=true --path=./src/app/i18n/locales",
    "i18n:sync": "locize sync --ver=latest --path=./src/app/i18n/locales --dry=true"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@prisma/client": "^4.15.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@tanstack/react-query": "^4.29.14",
    "@tanstack/react-query-devtools": "^4.29.14",
    "@tanstack/react-table": "^8.9.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "accept-language": "^3.0.18",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "i18next": "^23.2.8",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-locize-backend": "^6.2.2",
    "i18next-resources-to-backend": "^1.1.4",
    "lucide-react": "^0.233.0",
    "next": "13.4.4",
    "nodemailer": "^6.9.3",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-i18next": "^13.0.1",
    "tailwind-merge": "^1.13.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "5.1.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@commitlint/cli": "^17.6.5",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/nodemailer": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "babel-jest": "^29.5.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "locize-cli": "^7.14.6",
    "msw": "^1.2.2",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
